@model List<Resume>

<section id="resume" class="resume section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Özgeçmişim</h2>
        <p>Bilgisayar mühendisliği öğrencisiyim. Yazılım geliştirme alanında özellikle C# ve .NET teknolojilerine ilgi duyuyorum. Projelerimde temiz kod, takım çalışması ve kullanıcı deneyimi odaklı çözümler üretmeye çalışıyorum.</p>
    </div><!-- End Section Title -->

    <div class="container">

        <div class="row">

            <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">

                <h3 class="resume-title">Eğitimlerim</h3>
                @foreach (var item in Model)
                { 
                <div class="resume-item">
                    <h4>@item.Title</h4>
                    <h5>@item.ResumeDate</h5>
                    <p><em>@item.Subtitle</em></p>
                    <p>@item.Description</p>
                </div><!-- Edn Resume Item -->
                }

            </div>

            @await Component.InvokeAsync("_DefaultExperienceComponentPartial")



        </div>

    </div>

</section><!-- /Resume Section -->