@model portfolioCore.Entities.Resume

@{
    ViewData["Title"] = "CreateResume";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Eğitim Ekleme Sayfası</h2>
<form method="post">
    <label>Başlık</label>
    <input type="text" class="form-control" asp-for="Title" style="border:1px solid black;" />
    <br />
    <label>Eğitim Yılı</label>
    <input type="text" class="form-control" asp-for="ResumeDate" style="border:1px solid black;" />
    <br />
    <label>Bölüm</label>
    <input type="text" class="form-control" asp-for="Subtitle" style="border:1px solid black;" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" asp-for="Description" style="border:1px solid black;" />
    <br />
    <button class="btn btn-primary">Eğitim Oluştur</button>
</form>

